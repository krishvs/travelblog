<%= simple_form_for(@plan) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date_time %>
    <%= f.input :name %>
    <%= f.input :itenary_id %>
    <%= f.input :short_description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
